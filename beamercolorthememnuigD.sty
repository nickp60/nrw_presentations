% http://paletton.com/#uid=7050t0kkJkJsntwoyp6gYgoddc4

\definecolor{mDarkBrown}{HTML}{604c38}
\definecolor{mDarkTeal}{HTML}{23373b}
\definecolor{mDarkTeal2}{HTML}{183A59}
\definecolor{headgrey}{HTML}{272727}
\definecolor{footgrey}{HTML}{101010}
\definecolor{foottextgrey}{HTML}{AAAAAA}

\definecolor{mLightBrown}{HTML}{EB811B}
\definecolor{mLightBrown2}{HTML}{892075}
\definecolor{mMediumBrown}{HTML}{C87A2F}

\setbeamercolor{background canvas}{bg=black}
\setbeamercolor{normal text}{fg=white}
\setbeamercolor{alerted text}{fg=yellow}
\setbeamercolor{structure}{bg=footgrey, fg=foottextgrey}
\setbeamercolor{palette primary}{fg=mDarkBrown}
\setbeamercolor{palette secondary}{fg=mDarkBrown}
\setbeamercolor{palette tertiary}{fg=white, bg=mMediumBrown}
\setbeamercolor{palette quaternary}{fg=white, bg=headgrey}
\setbeamercolor{frametitle}{parent=palette quaternary}
\setbeamercolor{title}{parent=palette quaternary}

\setbeamercolor{page number in head/foot}{fg=foottextgrey}

\mode
<all>
